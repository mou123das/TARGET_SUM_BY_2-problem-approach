//TARGET-SUM-BY-2
import java.util.*;
class TARGET_SUM_BY_2
{
public void print()
{
Scanner sc =new Scanner(System.in);
System.out.println("ENTER THE LENGTH OF THE ARRAY");
int n=sc.nextInt();//Reading the length of the array from the user
int a[]=new int[n];
int sum=0,i,j,m=0,n1=0,v=0,q=0,k;
int b[]=new int[n];//creating a clone array of the pairs to check for the reoccurences

//Reading elements of the array
for(i=0;i<n;i++)
{
System.out.println("ENTER ELEMENT");
a[i]=sc.nextInt();
}
//Reading the integer
System.out.println("ENTER THE INTEGER");
int num=sc.nextInt();

System.out.println(num+" is expressed as a sum of the followings: ");
for(i=0;i<n;i++)
{

for(j=0;j<n;j++)
{

sum=a[j]+a[i];//evaluating the sum


if((sum==num) && (i!=j))//if sum of the numbers is equal to the integer and the indices are not same
{

for(k=0;k<(q-1);k++)
{

if((a[i]==b[k+1]) && (a[j]==b[k]))//to prevent printing out the same pair twice
{
v=1;
break;
}

}

if(v==0)
{
m=a[i];
n1=a[j];
b[q++]=m;
b[q++]=n1;


System.out.println(m+" , "+n1);
}
}

}
sum=0;
v=0;
}
}

}



